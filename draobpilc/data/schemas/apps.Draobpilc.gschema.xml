<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
    <schema path="/org/Draobpilc/" id="org.Draobpilc">

        <key type="i" name="width-percents">
            <default>40</default>
        </key>

        <key type="i" name="editor-width-percents">
            <default>85</default>
        </key>

        <key type="i" name="editor-height-percents">
            <default>85</default>
        </key>

        <key type="i" name="item-max-lines">
            <default>3</default>
        </key>

        <key type="i" name="item-max-height">
            <default>80</default>
        </key>

        <key type="i" name="item-preview-margin">
            <default>5</default>
        </key>

        <key type="i" name="kind-indicator-width">
            <default>3</default>
        </key>

        <key type="i" name="edit-timeout-ms">
            <default>500</default>
        </key>

        <key type="b" name="show-indexes">
            <default>true</default>
        </key>

        <key type="i" name="search-timeout">
            <default>300</default>
        </key>

        <key type="i" name="fuzzy-search-max-distance">
            <default>15</default>
        </key>

        <key type="b" name="startup-notification">
            <default>true</default>
        </key>

        <key type="s" name="merge-decorators">
            <default>
                '[
                    ["Space", " "],
                    ["Single quote", "\'"],
                    ["Double quote", "\\""],
                    ["Backslash", "\\\\"]
                ]'
            </default>
        </key>

        <key type="s" name="merge-separators">
            <default>
                '[
                    ["Newline", "\\n"],
                    ["Space", " "],
                    ["-", "-"]
                ]'
            </default>
        </key>

        <key type="s" name="merge-default-decorator">
            <default>''</default>
        </key>

        <key type="s" name="merge-default-separator">
            <default>'\n'</default>
        </key>

        <key type="i" name="show-editor-timeout">
            <default>500</default>
        </key>

        <key type="i" name="hide-editor-timeout">
            <default>1000</default>
        </key>

        <key type="s" name="gpaste-schema-id">
            <default>'org.gnome.GPaste'</default>
        </key>

        <key type="s" name="gpaste-dbus-name">
            <default>'org.gnome.GPaste'</default>
        </key>

        <key type="s" name="gpaste-dbus-path">
            <default>'/org/gnome/GPaste'</default>
        </key>

        <key type="s" name="gpaste-dbus-iface">
            <default>'org.gnome.GPaste1'</default>
        </key>

        <key type="b" name="show-text-info">
            <default>false</default>
        </key>

        <key type="s" name="show-histories">
            <default><![CDATA['<Ctrl>S']]></default>
        </key>

        <key type="s" name="delete-item">
            <default><![CDATA['Delete']]></default>
        </key>

        <key type="s" name="focus-search">
            <default><![CDATA['<Ctrl>F']]></default>
        </key>

        <key type="s" name="editor-wrap-text-shortcut">
            <default><![CDATA['<Ctrl>W']]></default>
        </key>

        <key type="s" name="open-item">
            <default><![CDATA['<Ctrl>Return']]></default>
        </key>

        <key type="s" name="backup-history">
            <default><![CDATA['<Ctrl>B']]></default>
        </key>

        <key type="s" name="keep-search-and-close">
            <default><![CDATA['<Ctrl>Escape']]></default>
        </key>

        <key type="s" name="hide-app">
            <default><![CDATA['Escape']]></default>
        </key>

        <key type="s" name="quit-app">
            <default><![CDATA['<Ctrl>Q']]></default>
        </key>

        <key type="b" name="show-thumbnails">
            <default>true</default>
        </key>

        <key type="b" name="editor-wrap-text">
            <default>true</default>
        </key>

        <key type="b" name="preview-text-files">
            <default>true</default>
        </key>

    </schema>
</schemalist>
